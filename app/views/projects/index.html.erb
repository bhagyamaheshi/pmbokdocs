<div id="new_project" class="col-md-4">
  <h3><%= link_to 'New Project', new_project_path %></h3>

</div>
<div id="all_projects" class="col-md-4">
  <div class="container">
    <div class="page-header">
      <h1>Listing projects</h1>
    </div>
    <br/>
    <div class="row">
      <table class="table">
        <thead>
        <tr>
          <th>Projectname</th>
          <th>Description</th>
          <th>Startdate</th>
          <th>Enddate</th>
          <th colspan="3"></th>
        </tr>
        </thead>

        <tbody>
        <% @projects.each do |project| %>
            <tr>
              <td><%= project.projectName %></td>
              <td><%= project.description %></td>
              <td><%= project.startDate %></td>
              <td><%= project.endDate %></td>
              <td><%= link_to 'Show', project %></td>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
  <br>
</div>

