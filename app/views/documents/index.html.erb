<div id="new_project" class="col-md-4">
  <%= link_to 'Back', projects_path, :class => "btn btn-default" %>
</div>

<div id="new_project" class="col-md-4">
  <div class="container">
    <div class="page-header">
      <h1>Listing documents</h1>
    </div>
    <table>
      <thead>
        <tr>
          <th>Category</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
        <% @document_categories.each do |documentCategory| %>
          <tr>
            <td><%= link_to documentCategory.categoryName,:controller => :documents, :action => :show,
                            :projectId => @projectId, :documentCategoryId => documentCategory.id %></td>
          </tr>
      <% end %>
    </tbody>
</table>

<br>
</div>
