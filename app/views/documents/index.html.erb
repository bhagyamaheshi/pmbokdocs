<div id="new_project" class="col-md-4">
  <h2><%= link_to 'New Document', new_document_path %></h2>
  <br/>
  <%= link_to 'Back', projects_path, :class => "btn btn-default" %>
</div>

<div id="new_project" class="col-md-4">
  <div class="container">
    <div class="page-header">
      <h1>Listing documents</h1>
    </div>
    <table>
      <thead>
        <tr>
          <th>Documentid</th>
          <th>Title</th>
          <th>Description</th>
          <th>Filelocation</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @documents.each do |document| %>
          <tr>
            <td><%= document.id %></td>
            <td><%= document.title %></td>
            <td><%= document.description %></td>
            <td><%= document.fileLocation %></td>
            <td><%= link_to 'Show', document %></td>
            <td><%= link_to 'Edit', edit_document_path(document) %></td>
            <td><% link_to 'Destroy', document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
      <% end %>
    </tbody>
</table>

<br>
</div>
