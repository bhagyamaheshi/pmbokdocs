<div id="new_project" class="col-md-4">
	<div class="container">
		<div class="page-header">
			<% @documentName.each do |name| %>
      			<h1><%= name.categoryName %></h1>
      		<% end %>
					<table class="table table-bordered">
      			<thead>
        			<tr>
          				<th>Version</th>
          				<th>Location</th>
        			</tr>
      			</thead>
      			<tbody>
        			<% @documentList.each do |list| %>
          			<tr>
            			<td><%= link_to "Version " + list.version.to_s,'#' %></td>
            			<td><%= list.fileLocation %></td>
          			</tr>
      				<% end %>
    			</tbody>
			</table>

			<%= link_to 'Upload', new_document_path(:projectId => params[:projectId], :documentCategoryId => params[:documentCategoryId]), :class => "btn btn-default" %>
 
    	</div>

      <%= form_for @uploadDocument, :url => document_path, :html => { :multipart => true } do |form| %>
        <% form.hidden_field(:projectId, params[:projectId]) %>
        <% form.hidden_field(:documentCategoryId, params[:documentCategoryId]) %>
        <%= form.file_field :file %>
      <% end %>

	</div>
</div>
