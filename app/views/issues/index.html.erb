<div id="new_project" class="col-md-4">
  <h3><%= link_to 'New Issue', new_issue_path %></h3>
  <br/>
  <%= link_to 'Back', projects_path, :class => "btn btn-default" %>
</div>
<div id="all_projects" class="col-md-6">
  <div class="container">
    <div class="page-header">
      <h1>Listing issues</h1>
    </div>
    <br/>
    <div class="row">
      <table class="table">
        <thead>
        <tr>
          <th>Issueid</th>
          <th>Issuename</th>
          <th colspan="3"></th>
        </tr>
        </thead>

        <tbody>
        <% @issues.each do |issue| %>
            <tr>
              <td><%= issue.id %></td>
              <td><%= issue.issueName %></td>
              <td><%= link_to 'Show', issue %></td>

            </tr>
        <% end %>
        </tbody>
      </table>

      <br>
    </div>
  </div>
</div>
